<h1>Listing duelos</h1>

<table>
  <tr>
    <th>Pessoa</th>
    <th>Data inicio</th>
    <th>Data fim</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @duelos.each do |duelo| %>
  <tr>
    <td><%= duelo.data_inicio %></td>
    <td><%= duelo.data_fim %></td>
    <td><%= link_to 'Show', duelo %></td>
    <td><%= link_to 'Edit', edit_duelo_path(duelo) %></td>
    <td><%= link_to 'Destroy', duelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

